{
  "name": "graphql-by-prisma",
  "scripts": {
    "start": "node dist/server",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only --watch src,graphql --exit-child -r tsconfig-paths/register src/server",
    "dev-check": "ts-node-dev --no-notify --respawn --watch src,graphql --exit-child -r tsconfig-paths/register src/server"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.6",
    "@graphql-tools/load": "^7.8.1",
    "@graphql-tools/schema": "9.0.4",
    "@prisma/client": "4.5.0",
    "express": "4.18.2",
    "express-graphql": "0.12.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "1.20.0"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/node": "18.11.7",
    "prisma": "4.5.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.8.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
